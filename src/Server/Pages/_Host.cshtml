@page "/"
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html theme="light">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>Valhalla Loot List</title>
    <base href="~/" />
    <link rel="apple-touch-icon" sizes="180x180" href="~/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/favicon-16x16.png">
    <link rel="manifest" href="~/app.webmanifest">
    <meta name="msapplication-TileColor" content="#00a300">
    <meta name="theme-color" content="#388E3C">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <link href="~/_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" asp-append-version="true" />
    <link href="~/app.css" rel="stylesheet" asp-append-version="true" />
</head>
<body>
    <component type="typeof(ValhallaLootList.Client.App)" render-mode="WebAssemblyPrerendered" />
    <div id="blazor-error-ui">
        An unhandled error has occurred.
        <a href="" class="reload">Reload</a>
        <a class="dismiss">Close</a>
    </div>
    <script src="~/_content/Microsoft.AspNetCore.Components.WebAssembly.Authentication/AuthenticationService.js"></script>
    <script src="~/_framework/blazor.webassembly.js"></script>
    <script>const whTooltips = {};</script>
    <script src="https://wow.zamimg.com/widgets/power.js"></script>
    <script src="~/_content/MudBlazor/MudBlazor.min.js" asp-append-version="true"></script>
    <script src="~/app.js" asp-append-version="true"></script>
</body>
</html>
